{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://mytgtodoist.local/schemas/command_envelope.schema.json",
  "title": "CommandEnvelope",
  "type": "object",
  "additionalProperties": true,
  "required": [
    "trace_id",
    "command"
  ],
  "properties": {
    "trace_id": {
      "type": "string",
      "minLength": 1
    },
    "source": {
      "type": "string"
    },
    "command": {
      "type": "object",
      "additionalProperties": true,
      "required": [
        "intent",
        "entities"
      ],
      "properties": {
        "intent": {
          "type": "string",
          "minLength": 1
        },
        "entities": {
          "type": "object"
        }
      }
    }
  }
}
